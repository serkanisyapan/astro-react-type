---
import { ReactType } from "../components/ReactType";
import { allWords } from "../data/words";
import { pickRandomWords } from "../utils/pickRandomWords";
import BaseLayout from "../layouts/BaseLayout.astro";
const randomWords = pickRandomWords(allWords, 30);
const pageTitle = "React Type";
const showFooter = true;
---

<BaseLayout pageTite={pageTitle} showFooter={showFooter}>
  <ReactType client:load randomWords={randomWords} />
</BaseLayout>
